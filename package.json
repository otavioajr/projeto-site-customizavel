{
  "name": "landing-page-aventuras",
  "version": "1.0.0",
  "description": "Landing page dinâmica para profissional de esportes de aventura com admin editável",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon dev-server.js",
    "dev:homol": "node -r dotenv/config server.js dotenv_config_path=.env.homol",
    "dev:prod": "node -r dotenv/config server.js dotenv_config_path=.env.production",
    "fast-dev": "node fast-dev.js",
    "vercel-dev": "vercel dev",
    "build": "echo 'No build needed'",
    "preview": "npx serve -p 3000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "landing-page",
    "static-site",
    "vercel",
    "admin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "hasown": "^2.0.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "serve": "^14.2.1",
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "supertest": "^7.0.0",
    "jsdom": "^24.0.0"
  }
}
